// multiplicando campos que existem
db.products.update(
  { _id: 1 },
  { $mul: { price: NumberDecimal("1.25"), qty: 2 } },
);

// multiplicando campos que nao existem
db.products.update(
  { _id: 2 },
  { $mul: { price: NumberLong("100") } },
);

// multiplicando valors com tipos diferentes
db.products.update(
  { _id: 3 },
  { $mul: { price: NumberInt(5) } },
);
